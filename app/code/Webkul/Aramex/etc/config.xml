<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <webkularamex>
			    <sandbox_mode>0</sandbox_mode>
                <active>0</active>
                <sallowspecific>0</sallowspecific>
                <handling>0</handling>
                <model>Webkul\Aramex\Model\Carrier</model>
                <title>Aramex Shipping</title>
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
                <unit_of_measure>KG</unit_of_measure>
                <max_package_weight>999</max_package_weight>
                <handling_type>F</handling_type>
                <handling_action>O</handling_action>
                <is_online>1</is_online>
            </webkularamex>
        </carriers>
        <payment>
            <webkularamex>
                <model>Webkul\Aramex\Model\CODPaymentMethod</model>
                <active>1</active>
                <title>Aramex COD</title>
                <order_status>pending</order_status><!-- set default order status-->
            </webkularamex>
        </payment>
    </default>
</config>
